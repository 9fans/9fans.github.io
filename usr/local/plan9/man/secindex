#!/usr/local/plan9/bin/rc
builtin cd $1
for (i in [a-z0-9:]*.[0-9]*) {
	b=`{echo $i | sed 's/\..*//'}
	9 sed -n '
	/SH *NAM/,/SH/{
		/SH/d
		s/, *$//
		ty
		:y
		s/ *\\*-.*//
		tx
		s/ *\\\(mi.*//
		tx
		s/, */\
/g
		s/\n\\n/\
/g
		s/$/ '$i'/g
		p
	}
	/SH *DES/q
	d
	:x
	s/ *\\*-.*//
	s/ *\\\(mi.*//
	/^$/d
	s/, */\
/g
	s/\n\n/\
/g
	s/$/ '$i'/g
	p
	q
' $i
echo $b $i
} |9 sort -u
